<h1>All Films</h1>
<table class="table">
  {{#each films}}
  <tr>
    <th>Title:</th>
    <td>{{Title}}</td>
  </tr>
  <tr>
    <th>Releases:</th>
    <td>{{Released}}</td>
  </tr>
  <tr>
    <th>Awords:</th>
    <td>{{Awards}}</td>
  </tr>
  <tr>
    <th>Actors:</th>
    <td>{{Actors}}</td>
  </tr>
  <tr>
    <th>Rated:</th>
    <td>{{Rated}}</td>
  <tr>
    <th>Runtime:</th>
    <td>{{Runtime}}</td>
  </tr>
   <tr>
    <th>Genre:</th>
    <td>{{Genre}}</td>
  </tr>
   <tr>
    <th>Director:</th>
    <td>{{Director}}</td>
  </tr>
  <tr>
    <th>Writer:</th>
    <td>{{Writer}}</td>
  </tr>
   <tr>
    <th>Plot:</th>
    <td>{{Plot}}</td>
  </tr>
  <tr>
    <th>Language:</th>
    <td>{{Language}}</td>
  </tr>
  <tr>
    <th>Country:</th>
    <td>{{Country}}</td>
  </tr>
   <tr>
    <th>Poster:</th>
    <td>{{Poster}}</td>
  </tr>
  <tr>
    <th>Metascore:</th>
    <td>{{Metascore}}</td>
  </tr>
   <tr>
    <th>imdbRating:</th>
    <td>{{imdbRating}}</td>
  </tr>
   <tr>
    <th>imdbVotes:</th>
    <td>{{imdbVotes}}</td>
  </tr>
  <tr>
    <th>Type:</th>
    <td>{{Type}}</td>
  </tr>
  <tr>
    <th>Images:</th>
    <td><img src="{{Images}}" alt="Film Photos" width="300" height="200">
    
    </td>
  </tr>
  {{/each}}
</table>